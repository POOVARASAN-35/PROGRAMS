import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      int rev,temp=n,sum=0;
      while(n>0){
        rev=n%10;
        sum=(sum*10)+rev;
        n=n/10;
      }
      if(temp==sum){
        System.out.print("YES");
      }else{
        System.out.print("NO");
      }
  }
}
