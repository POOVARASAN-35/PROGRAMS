import java.util.*;

public class Main {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int temp=n,a;
    int sum=0;
    while(n!=0){
      a=n%10;
      sum+=Math.pow(a,3);
      n=n/10;
    }
    if(sum==temp){
      System.out.println("YES");
    }
    else
    System.out.println("NO");
  }
}
